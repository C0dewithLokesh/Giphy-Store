{
  "expo": {
    "name": "Giphy Store",
    "slug": "GiphyStore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo2.png",
    "scheme": "giphystore",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.C0dewithLokesh.Gif"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/logo2.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "22e278f8-152d-432b-94b7-c17cb489c396"
      }
    }
  }
}
